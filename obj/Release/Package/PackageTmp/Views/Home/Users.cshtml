
@{
    ViewBag.Title = "Users";
}

@model List<EAIFMVC.Models.ContactorWrapper>

<div>
    <div class="container_12">

        <div class="clearfix">&nbsp;</div>

        <table width="100%" border="0" cellpadding="0" cellspacing="0">

            <tr>

                <th width="18%" scope="col">姓名</th>
                <th width="25%" scope="col">所属公司</th>
                <th width="29%" scope="col">联系电话</th>
            </tr>

            @foreach (var alert in Model)
            {
                <tr>
                    <td scope="col">@alert.Name</td>
                    <td scope="col">@alert.CompanyName</td>
                    <td scope="col">@alert.Phone</td>
                </tr>
            }
        </table>
    </div>
</div>

